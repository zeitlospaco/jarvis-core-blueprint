# Example environment variables (copy to .env and fill)
# Do NOT commit your real .env

# n8n auth
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=securepass123
N8N_ENCRYPTION_KEY=randomgeneratedkey

# Supabase (explicit names added)
SUPABASE_DB_HOST=db.xxxxx.supabase.co
SUPABASE_SERVICE_ROLE=your_supabase_service_role_secret
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key

# LLM & Vector DB
OPENAI_API_KEY=your_openai_key
ANTHROPIC_API_KEY=your_anthropic_key
PINECONE_API_KEY=your_pinecone_key
PINECONE_ENV=us-east1-gcp

# Postgres (for local docker-compose)
POSTGRES_DB=n8n
POSTGRES_USER=n8n_user
POSTGRES_PASSWORD=yoursecurepostgrespassword

# Traefik / Let's Encrypt
ACME_EMAIL=your-email@example.com
TRAEFIK_BASIC_AUTH=admin:$apr1$xyz...

# SMTP (for alerts)
SMTP_HOST=smtp.example.com
SMTP_PORT=587
SMTP_USER=your-smtp-user
SMTP_PASSWORD=your-smtp-password
SMTP_FROM_EMAIL=noreply@your-domain.com

# Timezone
TZ=Europe/Berlin
