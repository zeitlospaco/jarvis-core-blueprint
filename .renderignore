scripts/

# Operations agent (not used on Render)
ops_agent.yml

# Node modules (if any)
node_modules/

# Python cache
__pycache__/
*.pyc
*.pyo
*.pyd
.Python

# IDE
.vscode/
.idea/
*.swp
*.swo
*~

# OS files
.DS_Store
Thumbs.db

# Logs
*.log
logs/

# Test files
tests/
test/
*.test.js
*.spec.js

# Build artifacts
dist/
build/
*.tar.gz
*.zip

# Database files (local only)
postgres/
*.sql
*.db

# Backups
backups/
*.backup
